
@{
    ViewBag.Title = "Index";
}



<h2>Index</h2>

<div class="row">
    <div class="small-12 medium-12 large-12 columns">
        <a href="#" data-reveal-id="myModal">Click Me For A Modal</a>

        <div id="myModal" class="tiny reveal-modal" data-reveal aria-labelledby="modalTitle" aria-hidden="true" role="dialog">
            <h2 id="modalTitle">Awesome. I have it.</h2>
            <p class="lead">Your couch.  It is mine.</p>
            <p>I'm a cool paragraph that lives inside of an even cooler modal. Wins!</p>
            <a class="close-reveal-modal" aria-label="Close">&#215;</a>
            <a href="#" class="small radius button right">Default Button</a>
        </div>
        <a href="#" class="small radius button right" onclick="App.ToggleModal()">Default Button</a>
    </div>
</div>